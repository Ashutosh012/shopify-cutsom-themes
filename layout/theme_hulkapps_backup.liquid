<body class="{{ non_background }} template-collection {% if template == "index" %}index-template{% endif %} {% if customer %}loggedin{% endif %} april_15 " >

  {% if settings.mailing_list_active %}
  {% include 'newsletter-popup' %}  
  {% endif %}
  {% if settings.general_loadingscreen %}
  <!-- Preloading -->
  <div id="loader-div">
    <div id="loader-wrapper">
      <div class="loading-div-logo">
        {% if settings.general_loadingscreen_logo %}
        <img src="{{ settings.general_loadingscreen_logo | img_url: "", crop: '' | split:'?v=' | first }}" alt="loading svg" />
        {% endif %}
        <div id="loader"><img src="{{'ring.svg' | asset_url}}" alt="loading svg" /></div>
      </div>     
    </div>    
  </div>
  {% endif %}
  <!-- Header -->  
  <header id="top" class="header clearfix">
    {% if settings.header_id == 'header_1' %}
    {% section 'theme-header' %}
    {% elsif settings.header_id == 'header_2' %}
    {% section 'theme-header-2' %}
    {% elsif settings.header_id == 'header_3' %}
    {% section 'theme-header-3' %}
    {% elsif settings.header_id == 'header_4' %}
    {% section 'theme-header-4' %}
    {% elsif settings.header_id == 'header_5' %}
    {% section 'theme-header-5' %}
    {% else %}
    {% section 'theme-header-6' %}
    {% endif %}
  </header>
  <div class="fix-sticky"></div>

  <!-- Main Content -->
  <div class="page-container" id="PageContainer">
    <main class="main-content" id="MainContent" role="main">
      {{ content_for_layout }}
    </main>
  </div>

  <!-- Footer -->
  <footer class="footer">
    {% if settings.footer_id == 'footer_1' %}
    {% section 'theme-footer' %} 
    {% elsif settings.footer_id == 'footer_2' %}
    {% section 'theme-footer-2' %} 
    {% elsif settings.footer_id == 'footer_3' %}
    {% section 'theme-footer-3' %} 
    {% elsif settings.footer_id == 'footer_4' %}
    {% section 'theme-footer-4' %} 
    {% else %}
    {% section 'theme-footer-5' %} 
    {% endif %}
  </footer>

  <!-- Float right icon -->
  <div class="float-right-icon">
    <ul>
      <li>
        {% if settings.general_gototop %}
        <div id="scroll-to-top" data-toggle="" data-placement="left" title="Scroll to Top" class="off">
          <i class="fa fa-angle-up"></i>
        </div>
        {% endif %} 
      </li>
    </ul>
  </div>
  
  {% if shop.url contains 'cs-april' %}
    {% include 'home-listing' %}
  {% endif %}

  <script type="text/javascript">
    var qs_quantity = '', wishlistpage = 0, comparepage = 0, checkCollection = 0, checkProductV3 = 0, checkProduct = 0;
    {% if template contains 'wish-list'%}wishlistpage=1;{% endif %}
    {% if template contains 'compare'%}comparepage=1;{% endif %}
    {% if template contains 'collection' %}
    checkCollection=1;
    {% endif %}
    {% if template contains 'collection' and template contains 'sidebar' %}checkCollection=2;{% endif %}
    {% if settings.collection_style == 'full_sidebar' or settings.collection_style == 'right_sidebar' or settings.collection_style == 'grid_sidebar' %}
      {% if template == 'collection' %}
      checkCollection=2;
      {% endif %}
    {% endif %}
    {% if template contains 'product' %}
      checkProduct = 1;
    {% endif %}
    {% if template contains 'product.v3' %}
      checkProductV3 = 1;
    {% elsif template == 'product' and settings.product_style == 'v_3' %}
      checkProductV3 = 1;
    {% endif %}
    var cart_money_format={{ shop.money_format | strip_html | json }};
    var quickShop_money_format="<span class='money'>"+{{ shop.money_format | strip_html | json }}+"</span>";

