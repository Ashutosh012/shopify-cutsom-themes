<div class="contact-hero-section">
  <div class="contact-hero-wrap">
  <div class="contact-hero-left">
    {% if section.settings.image_left_first != blank %}
      <div class="conatct-hero-img">
        <img src="{{ section.settings.image_left_first | img_url: "1024x1024"  }}" alt="conatct-hero-img">
      </div>
    {% else %}
    	<div class="conatct-hero-img">
          <img src="https://cdn.shopify.com/s/files/1/0506/1450/1551/files/home_slider_5_900x.jpg" alt="conatct-hero-img">
        </div>
   	{% endif %}
    {% if section.settings.image_left_second != blank %}
      <div class="conatct-hero-img contact-hero-img-two">
        <img src="{{ section.settings.image_left_second | img_url: "1024x1024"  }}" alt="conatct-hero-img">
      </div>
    {% else %}
    	<div class="conatct-hero-img contact-hero-img-two">
          <img src="https://cdn.shopify.com/s/files/1/0506/1450/1551/files/home_slider_5_900x.jpg" alt="conatct-hero-img">
        </div>
    {% endif %}
  </div>
    <div class="contact-hero-text page-width text-{{ section.settings.text_align }}">
      {% if section.settings.title != blank %}
      <h1 class="h1 contact-text-title {% if section.settings.text_size == 'medium' %} mega-title--medium{% endif %}">{{ section.settings.title | replace:'uns', '<br>uns' }}</h1>
      {% endif %}
    </div>
    {% if section.settings.image_right != blank %}
      <div class="contact-hero-right">
        <div class="conatct-hero-img">
          <img src="{{ section.settings.image_right | img_url: "large" }}" alt="conatct-hero-img">
        </div>
      </div>
    {% else %}
      <div class="contact-hero-right">
        <div class="conatct-hero-img">
          <img src="https://cdn.shopify.com/s/files/1/0506/1450/1551/files/home_slider_5_900x.jpg" alt="conatct-hero-img">
        </div>
      </div>	
   	{% endif %}
  </div>
</div>

