    .slideshow-categoeries--index .slide-content{
      height: 350px;
    }
    .slideshow-categoeries--index .slide-img{
      left: 15px;
    }
    .slideshow-categoeries--index .slider-heading{
      font-size: 40px;
    }
    .slideshow-categoeries--index .set-content{
      padding: 0px 20px 0px 45px;
    }
  }
  .slideshow-categoeries--index .slick-dots {
    left: 20%;
    position: absolute;
    margin-top: -40px;
  }
