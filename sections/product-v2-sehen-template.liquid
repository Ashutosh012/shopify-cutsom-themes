    position: relative;
    vertical-align: middle;
    height: inherit;
    width: inherit;
    display: table-cell;
    padding: 0px 20px 0px 70px;
    text-align: {{ section.settings.text_align }};
  }
  .slideshow-categoeries--index .slider-heading{
    font-size: 50px;
    font-weight: 500;
    text-transform: uppercase;
    color: {{ section.settings.title_text_color}}
  }
