    $('div.collection-search-icon').click(function(){
      $(".collection-product-static img.grid-view-item__image").css("transform","scale(1.5)");
      $(".collection-product-hover img.secondary").css("transform","scale(1.5)");
    })
  })
</script>
<style>
  .slideshow-categoeries--index .slider-heading{
